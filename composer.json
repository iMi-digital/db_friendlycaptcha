{
	"name": "balatd/db_friendlycaptcha",
	"type": "typo3-cms-extension",
	"description": "Bot protection for the internet.",
	"license": "GPL-2.0-or-later",
	"version": "1.0.0",

	"support": {
		"issues": "https://github.com/balatD/db_friendlycaptcha/issues"
	},

	"keywords": [
		"friendly captcha"
	],

	"suggest": {
		"typo3/cms-form": "^10.0",
		"in2code/femanager": "^6"
	},

	"require": {
		"typo3/cms-core": "^10.0"
	},

	"autoload": {
		"psr-4": {
			"BalatD\\FriendlyCaptcha\\": "Classes/"
		}
	},

	"config": {
		"vendor-dir": ".Build/Web/vendor",
		"bin-dir": ".Build/bin"
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/Web"
		}
	}
}
